{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "hide-files.files": [
    "package-lock.json",
    "postcss.config.mjs",
    "README.md",
